#[autospy::autospy]
trait MyTrait {
    fn function(&self, ip: [u8; 4]);
}
#[derive(Default, Clone)]
struct MyTraitSpy {
    pub function: autospy::SpyFunction<IpAddr, ()>,
}
impl MyTrait for MyTraitSpy {
    fn function(&self, ip: [u8; 4]) {
        self.function.spy(ip.into())
    }
}
