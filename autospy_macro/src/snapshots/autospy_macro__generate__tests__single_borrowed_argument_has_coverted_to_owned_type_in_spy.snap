trait TestTrait {
    fn function(&self, argument: &str);
}
#[derive(Default, Clone)]
struct TestTraitSpy {
    pub function: autospy::SpyFunction<<str as ToOwned>::Owned, ()>,
}
impl TestTrait for TestTraitSpy {
    fn function(&self, argument: &str) {
        self.function.spy((argument).to_owned())
    }
}
