#[autospy::autospy]
trait MyTrait {
    fn function(&self, bytes: Vec<u8>);
}
#[derive(Default, Clone)]
struct MyTraitSpy {
    pub function: autospy::SpyFunction<Result<String, Utf8Error>, ()>,
}
impl MyTrait for MyTraitSpy {
    fn function(&self, bytes: Vec<u8>) {
        self.function.spy(String::from_utf8(bytes))
    }
}
